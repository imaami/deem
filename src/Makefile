override SRCS := deem.c utf8.c
override OBJS := $(SRCS:%=%.o)

-load deem.so
deem.so: $(OBJS)
	$(CC) $(CFLAGS) -shared -fPIC -o $@ $^

$(register-msg YEET,38;5;191)
override Y_ := (╯°□°）╯︵ ┻━┻
override Y__ = $(SGR 38;5;119,    $(Y_))
.PHONY: yeet
yeet:; $(msg YEET,$(Y__))@:

deem.so: | yeet
%.c.o: %.c
	$(CC) $(CFLAGS) -c -o $@ $<

